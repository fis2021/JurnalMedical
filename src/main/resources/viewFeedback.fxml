
<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10" prefWidth="600" prefHeight="400"
          xmlns="http://javafx.com/javafx/8.0.172-ea"
          fx:controller="org.loose.fis.sre.controllers.ViewFeedbackController">

    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
    </padding>
    <Text id="title-text" text="Feedback:" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="5"/>

    <Text fx:id="feedbackMessage" GridPane.rowIndex="1"  GridPane.columnSpan="5"/>
    <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="4" GridPane.rowIndex="2">
        <Button onAction="#handleBackAction" text="Back"/>
    </HBox>
</GridPane>

